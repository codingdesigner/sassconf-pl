{{#keynotes}}
  &lt;article class=&quot;session keynote&quot;&gt;
    {{#session-title}}
      &lt;div class=&quot;title&quot;&gt;
        &lt;h1&gt;{{session-title}}&lt;/h1&gt;
      &lt;/div&gt;
    {{/session-title}}
    &lt;div class=&quot;images&quot;&gt;
      {{#speakers}}
        &lt;div class=&quot;speaker-image&quot;&gt;&lt;img src=&quot;{{speaker_img_path}}{{speaker-image}}&quot; alt=&quot;{{alt}}&quot;&gt;&lt;/div&gt;
      {{/speakers}}
    &lt;/div&gt;
    &lt;div class=&quot;people&quot;&gt;
      {{#speakers}}
        &lt;div class=&quot;person&quot;&gt;
          &lt;h3 class=&quot;speaker&quot;&gt;{{name}}&lt;/h3&gt;
          &lt;h3 class=&quot;occupation&quot;&gt;{{occupation}}&lt;/h3&gt;
        &lt;/div&gt;
      {{/speakers}}
    &lt;/div&gt;
    {{#session-description}}
      &lt;div class=&quot;section--details base--STYLED&quot;&gt;
        &lt;div class=&quot;section-details--description&quot;&gt;{{{session-description}}}&lt;/div&gt;
      &lt;/div&gt;
    {{/session-description}}
  &lt;/article&gt;
{{/keynotes}}
